  <form>
    <div class="form-group">
      <label for="nombreJugador">Nombre</label>
      <input type="text" class="form-control" id="nombreEquipo" placeholder="Lebron James" required>
    </div>
    <div class="form-group">
      <label for="posicionJugador">Abreviatura</label>
      <input type="text" class="form-control" id="abreviaturaEquipo" placeholder="Base">
    </div>
    <div class="form-group">
      <label for="numeroJugador">Imagen</label>
      <input type="file" class="form-control" id="imagenEquipo">
    </div>
    <button id="crear" class="btn btn-default center-block">Nuevo</button>
  </form>
